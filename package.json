{
  "name": "astro-physics-engine",
  "private": false,
  "version": "0.2.0",
  "description": "A TypeScript library for N-body gravitational physics simulation with 2D & 3D vector math, conservation laws, and orbital mechanics",
  "keywords": ["physics", "gravity", "simulation", "n-body", "orbital-mechanics", "typescript", "celestial", "3d-physics", "angular-momentum", "conservation-laws", "vector-math"],
  "author": "Mateo Estrada",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MateoEstradaOSU/astro-engine.git"
  },
  "type": "module",
  "main": "./dist/astro-engine.umd.cjs",
  "module": "./dist/astro-engine.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/astro-engine.js",
      "require": "./dist/astro-engine.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "test": "./run-tests.sh",
    "test:compile": "npx tsc src/test-*.ts --outDir build --module es2020 --target es2020 --moduleResolution node --esModuleInterop"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4"
  }
}
